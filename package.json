{
  "name": "@metapages/metapage-grid-react",
  "version": "0.2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/metapages/metapage-grid-react.git"
  },
  "keywords": [
    "metapage",
    "react",
    "grid-react",
    "metaframe",
    "embed"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.js",
      "default": "./dist/components/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/index.d.ts"
      ],
      "components": [
        "./dist/components/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "browser": "./dist/index.js",
  "homepage": "https://github.com/metapages/metapage-grid-react/tree/main/docs#readme",
  "scripts": {
    "note": "echo 'use just. See README.md'"
  },
  "dependencies": {
    "object-hash": "^3.0.0",
    "react-grid-layout": "^1.5.0"
  },
  "peerDependencies": {
    "@metapages/metapage-react": "0.x",
    "@metapages/metapage": "1.x",
    "react": "18.x"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^12.1.1",
    "@types/node": "^20.17.7",
    "@types/object-hash": "^3.0.6",
    "@types/react": "^18.3.12",
    "@types/react-grid-layout": "^1.3.5",
    "react": "^18.3.1",
    "rollup-plugin-typescript-paths": "^1.5.0",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2",
    "vite": "^5.4.11"
  },
  "browserslist": {
    "production": [
      "last 3 and_chr versions",
      "last 3 chrome versions",
      "last 3 opera versions",
      "last 3 ios_saf versions",
      "last 3 safari versions"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
